{
  "require-dev": {
    "phpunit/phpunit": "^7",
    "symfony/var-dumper": "^4.2"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --configuration phpunit.xml"
  },
  "require": {
    "twig/twig": "~2.0",
    "narthur/natlib": "@dev"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "narthur/natlib",
        "version": "2019.06.20",
        "dist": {
          "type": "zip",
          "url": "https://github.com/narthur/natlib/archive/master.zip"
        }
      }
    },
    {
      "type": "path",
      "url": "../natlib",
      "options": {
        "symlink": true
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "Avorg\\": [
        "class/",
        "interface/",
        "test/",
        "test/stubs/"
      ]
    }
  }
}
